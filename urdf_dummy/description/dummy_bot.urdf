<?xml version="1.0" ?>
<robot name="dummy_bot">
  <link name="chassis">
    <visual>
      <geometry>
        <box size="1.0 0.7 0.5"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.085" ixy="0.0" ixz="0.0" iyy="0.04" iyz="0.0" izz="0.124"/>
    </inertial>
  </link>

  <link name="left_wheel">
    <visual>
      <geometry>
        <cylinder length="0.3" radius="0.2"/>
      </geometry>
      <origin rpy="1.57075 0 0" xyz="0 0.4 -0.3"/>
    </visual>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.0175" ixy="0.0" ixz="0.0" iyy="0.0175" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>
  <joint name="chassis_to_left_wheel" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.79769e+308" upper="1.79769e+308" velocity="0.5"/>
    <parent link="chassis"/>
    <child link="left_wheel"/>
  </joint>

  <link name="right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.3" radius="0.2"/>
      </geometry>
      <origin rpy="-1.57075 0 0" xyz="0 -0.4 -0.3"/>
    </visual>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.0175" ixy="0.0" ixz="0.0" iyy="0.0175" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>
  <joint name="chassis_to_right_wheel" type="fixed">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.79769e+308" upper="1.79769e+308" velocity="0.5"/>
    <parent link="chassis"/>
    <child link="right_wheel"/>
  </joint>

  

  

 
    
 
</robot>

